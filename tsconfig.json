{
  "ts-node": {
    "transpileOnly": true
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "es2016",
    "module": "commonjs",       
    "resolveJsonModule": true,
    "esModuleInterop": true,                       
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "strict": true,
    "noEmit": true                     
  }
}
